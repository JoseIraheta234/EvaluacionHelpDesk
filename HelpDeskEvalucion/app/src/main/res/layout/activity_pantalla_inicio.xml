<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Pantalla_Inicio">

  <EditText
      android:id="@+id/txtNumeroTicket"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:ems="10"
      android:hint="Numero Ticket"
      android:inputType="text"
      tools:layout_editor_absoluteX="34dp"
      tools:layout_editor_absoluteY="35dp" />

  <EditText
      android:id="@+id/txtTituloTicket"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:ems="10"
      android:hint="Titulo Ticket"
      android:inputType="text"
      tools:layout_editor_absoluteX="35dp"
      tools:layout_editor_absoluteY="95dp" />

  <EditText
      android:id="@+id/txtDescripcionTicket"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:ems="10"
      android:hint="Descripcion Ticket"
      android:inputType="text"
      tools:layout_editor_absoluteX="35dp"
      tools:layout_editor_absoluteY="156dp" />

  <EditText
      android:id="@+id/txtAutorTicket"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:ems="10"
      android:hint="Autor del ticket"
      android:inputType="text"
      tools:layout_editor_absoluteX="36dp"
      tools:layout_editor_absoluteY="231dp" />

  <EditText
      android:id="@+id/txtEmailContacto"
      android:layout_width="272dp"
      android:layout_height="43dp"
      android:ems="10"
      android:hint="Email de contacto de autor"
      android:inputType="text"
      tools:layout_editor_absoluteX="39dp"
      tools:layout_editor_absoluteY="303dp" />

  <EditText
      android:id="@+id/txtFechaCreacionTic"
      android:layout_width="265dp"
      android:layout_height="45dp"
      android:ems="10"
      android:hint="Fecha de Creacion de ticket"
      android:inputType="text"
      tools:layout_editor_absoluteX="42dp"
      tools:layout_editor_absoluteY="381dp" />

  <EditText
      android:id="@+id/txtEstadoTicket"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:ems="10"
      android:hint="EstadoTicket"
      android:inputType="text"
      tools:layout_editor_absoluteX="44dp"
      tools:layout_editor_absoluteY="458dp" />

  <EditText
      android:id="@+id/txtFechaFinalizacion"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:ems="10"
      android:hint="Fecha de Finalizacion"
      android:inputType="text"
      tools:layout_editor_absoluteX="45dp"
      tools:layout_editor_absoluteY="532dp" />

  <Button
      android:id="@+id/btnEnviar"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:text="Enviar"
      tools:layout_editor_absoluteX="151dp"
      tools:layout_editor_absoluteY="623dp" />
</androidx.constraintlayout.widget.ConstraintLayout>